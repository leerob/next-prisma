{
  "name": "prisma-next",
  "description": "Static site with Next.js 9.3 and Prisma.",
  "version": "0.0.0-dripip",
  "author": {
    "name": "Lee Robinson",
    "email": "me@leerob.io",
    "url": "https://leerob.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williamluke4/prisma-next.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "db": "prisma studio --experimental",
    "db-save": "prisma migrate save --experimental",
    "db-up": "prisma migrate up --experimental",
    "push": "prisma migrate save --experimental --name Dev && prisma migrate up --experimental && prisma generate",
    "remove-migrations": "sqlite3 prisma/dev.db 'delete from _Migration'",
    "generate": "prisma generate"
  },
  "dependencies": {
    "eslint-plugin-next-prisma": "file:../",
    "@chakra-ui/core": "0.8.0",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@prisma/client": "2.7.0",
    "chokidar": "^3.4.2",
    "download": "^8.0.0",
    "emotion-theming": "10.0.27",
    "next": "^9.5.3",
    "next-prisma-plugin": "^0.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "swr": "^0.3.2",
    "tempy": "^0.7.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.7.0",
    "@types/download": "^6.2.4",
    "@types/node": "^14.10.1",
    "typescript": "^4.0.2"
  }

{
  "name": "prisma-next",
  "description": "Static site with Next.js 9.3 and Prisma.",
  "version": "0.0.0-dripip",
  "author": {
    "name": "Lee Robinson",
    "email": "me@leerob.io",
    "url": "https://leerob.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williamluke4/prisma-next.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "db": "dotenv -- prisma studio --experimental",
    "doctor": "dotenv -- prisma doctor",
    "db:save": "dotenv -- prisma migrate save --experimental --name dev",
    "db:up": "dotenv -- prisma migrate up --experimental",
    "db:generate": "dotenv -- prisma generate",
    "push": "yarn db:save & yarn db:up && yarn db:generate"
  },
  "dependencies": {
    "@chakra-ui/core": "0.8.0",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@prisma/client": "2.9.0-integration-nextjs-custom-output.2",
    "chokidar": "^3.4.2",
    "download": "^8.0.0",
    "emotion-theming": "10.0.27",
    "klona": "^2.0.4",
    "next": "^9.5.3",
    "next-auth": "^3.1.0",
    "next-prisma-plugin": "^0.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "swr": "^0.3.5",
    "tempy": "^0.7.1"
  },
  "devDependencies": {
    "dotenv-cli": "^4.0.0",
    "@prisma/cli": "2.9.0-integration-nextjs-custom-output.2",
    "@types/download": "^6.2.4",
    "@types/next-auth": "^3.1.8",
    "@types/node": "^14.11.2",
    "typescript": "^4.0.3"
  }
}
